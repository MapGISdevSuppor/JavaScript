@charset "utf-8";
/*清除原有样式*/
html, body, #zdims { margin: 0; width: 100%; height: 100%; overflow:hidden;}

#logoBtn, #logoText{position:absolute; top:16px;left:16px; width:90px; height:90px;z-index:1000;}
#logoText{left:76px;width:420px;z-index:999;}
/* logo图标 */
.logoImg{ background-image: url(../images/sampleImg/logo.png); }
.logoBtn{width:60px; height:60px; background-position:0 0;cursor: pointer;}
.logoText{width:378px; height:75px; background-position:-60px 0;}

#main
{
	 width: 100%; height: 100%; overflow:hidden;
	 position:absolute;
	 top:0;
	 left:0;
}
#busStopBox
{
	padding:5px;
	padding-top:10px;
	background-color:white;
	position:absolute;
	right:10px;top:100px;
	display:none;
	z-index:900;
	width:220px;
	
				font-family:Microsoft YaHei,Times New Roman, "Helvetica Neue","lucida Grande";
    	font-size: 13px;
	color:black;
	
		border:5px solid #c8e9f0;
	
	    -moz-border-radius: 5px; /* Firefox */
    -webkit-border-radius: 5px; /* Safari Chrome */
    -moz-box-shadow: 3px 3px 5px #a7a7a7; /* Firefox */
    -webkit-box-shadow: 3px 3px 5px #a7a7a7; /* Safari 和 Chrome */
    
    box-shadow: 3px 3px 5px #a7a7a7; /* Opera 10.5+, IE6+ IE-CSS3 */
    border-radius: 5px; /* Opera 10.5+,  Internet Explorer 6+  IE-CSS3 */
}
