/* CSS Document */
*{ margin:0px; padding:0px;}
img{ border:0px;}
ul{ list-style:none;}
body,html{ font:12px "宋体"; height:100%;}
#warp{ width:100%; height:100%; overflow:hidden; position:relative;}
#suggest{
     width:100px;
     border:1px solid black ;
     font-size:14px ;
     z-index:101;
}
div.out{
    border:1px solid #FFFFFF ;
    background :#FFFFFF;
}
div.over{
    border:1px solid #999 ;
    background :#FFFFCC ;
    cursor:hand; 
}
#header{ width:100%; height:110px; overflow:hidden; background:url(../images/header.jpg) no-repeat;}
#top_rearch{ position:absolute;left:635px;top:5px;}
#top_rearch ul li{ float:left;}
#top_rearch a:link { color: #78B002; cursor:pointer;}
#top_rearch a:visited { color: #78B002; cursor:pointer;}
#top_rearch a:hover { color: #EA9312; cursor:pointer;}
#top_rearch a:active { color: #2863F8; cursor:pointer;}
#nav{ background:url(../images/nav_bg.gif) repeat-x; width:100%; height:39px; overflow:hidden; }

